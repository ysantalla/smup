{
  "name": "server",
  "version": "1.0.0",
  "description": "Starter repository",
  "license": "MIT",
  "scripts": {
    "nest": "nest",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "npm run build && node dist/main",
    "lint": "eslint '{src,apps,libs,test}/**/*.ts' --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.8",
    "@nestjs/config": "^0.2.2",
    "@nestjs/core": "^6.11.8",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.3.1",
    "@nestjs/platform-express": "^6.11.7",
    "@nestjs/swagger": "^4.2.8",
    "bcrypt": "^5.0.0",
    "cache-manager": "^2.11.1",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "express-rate-limit": "^5.1.1",
    "helmet": "^3.21.2",
    "mongoose": "^5.9.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "~6.5.4",
    "swagger-ui-express": "^4.1.3"
  },
  "devDependencies": {
    "@nestjs/cli": "~6.14.2",
    "@nestjs/schematics": "~6.9.3",
    "@nestjs/testing": "~6.11.7",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "~4.17.2",
    "@types/mongoose": "^5.7.2",
    "@types/node": "~10.17.3",
    "@types/supertest": "~2.0.8",
    "@typescript-eslint/eslint-plugin": "~2.19.2",
    "@typescript-eslint/parser": "~2.19.2",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.10.0",
    "eslint-plugin-import": "~2.20.1",
    "jest": "~25.1.0",
    "prettier": "~1.19.1",
    "supertest": "~4.0.2",
    "ts-jest": "~25.2.0",
    "ts-loader": "~6.2.1",
    "ts-node": "~8.6.2",
    "tsconfig-paths": "~3.9.0",
    "typescript": "~3.7.2"
  }
}
